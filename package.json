{
  "name": "blw-agricultural-market-data-platform",
  "version": "0.1.0",
  "repository": "git@github.com:interactivethings/blw-agricultural-market-data-platform.git",
  "author": "Jeremy Stucki <jeremy@interactivethings.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "lingui compile && next build",
    "export": "lingui compile && next build && next export -o build",
    "start": "next start -p $PORT",
    "lint": "eslint .",
    "typecheck": "tsc",
    "locales:extract": "NODE_ENV=development lingui extract",
    "locales:compile": "lingui compile",
    "graphql:codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "graphql:codegen:dev": "graphql-codegen --config codegen.yml --watch -r dotenv/config",
    "setup:dev": "yarn",
    "preversion": "yarn lint",
    "postversion": "git push --follow-tags",
    "icons:transform": "svgr -d src/icons/icons-jsx src/icons/icons-svg --replace-attr-values '#000=currentColor' --icon",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/standalone": "^7.8.4",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@interactivethings/swiss-federal-ci": "^0.1.0",
    "@juggle/resize-observer": "^3.0.2",
    "@lingui/react": "^3.13.2",
    "@mdx-js/react": "1.6.22",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.4",
    "@next/bundle-analyzer": "13.4.4",
    "@next/mdx": "13.4.4",
    "@urql/exchange-graphcache": "^4.4.2",
    "catalog": "^4.0.1-canary.2",
    "core-js": "^3.6.5",
    "d3": "^6.7.0",
    "framer-motion": "^6.2.8",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "jotai": "^1.6.5",
    "lodash": "^4.17.15",
    "next": "^13.4.4",
    "powerbi-client": "^2.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^8.26.1",
    "sparql-http-client": "^2.4.0",
    "tss-react": "^3.6.2",
    "urql": "^2.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-resolvers": "^2.5.2",
    "@graphql-codegen/typescript-urql": "^3.5.3",
    "@lingui/cli": "^3.13.2",
    "@lingui/macro": "^3.13.2",
    "@mdx-js/loader": "^1.6.22",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@svgr/cli": "^6.2.1",
    "@types/d3": "^6.7.0",
    "@types/lingui__core": "^3.0.0",
    "@types/lingui__macro": "^3.0.0",
    "@types/lingui__react": "^3.0.0",
    "@types/lodash": "^4.14.138",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^17.0.21",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "@urql/devtools": "^2.0.3",
    "babel-eslint": "10.x",
    "babel-plugin-macros": "^3.1.0",
    "dotenv": "^16.0.1",
    "eslint": "8.10.0",
    "eslint-config-next": "^13.4.4",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.29.3",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2"
  }
}
